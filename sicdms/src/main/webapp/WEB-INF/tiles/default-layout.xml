		<put-attribute name="article" 		value="/WEB-INF/views/login/hrscRplcKeyPopup.jsp" />
	</definition>
 -->

	<!-- 메인폴더  -->
    <definition name="main/**" template="/WEB-INF/tiles/empty.jsp">
        <put-attribute name="content" value="/WEB-INF/views/main/{1}.jsp" />
    </definition>

    <!-- 메인폴더  -->

	<!-- 일괄 처리 항목 리스트 -->
<!-- 	<definition name="*" extends="base"> -->
<!--         <put-attribute name="article" value="/WEB-INF/views/{1}.jsp" /> -->
<!--     </definition> -->



 	<definition name="*/*Tab" template="/WEB-INF/tiles/empty.jsp">
  		<put-attribute name="content" value="/WEB-INF/views/{1}/{2}Tab.jsp" />
	</definition>

 	<definition name="*/*/*Tab" template="/WEB-INF/tiles/empty.jsp">
  		<put-attribute name="content" value="/WEB-INF/views/{1}/{2}/{3}Tab.jsp" />
	</definition>


 	<definition name="*/*Detail" template="/WEB-INF/tiles/empty.jsp">
  		<put-attribute name="content" value="/WEB-INF/views/{1}/{2}Detail.jsp" />
	</definition>


 	<definition name="*/*/*Detail" template="/WEB-INF/tiles/empty.jsp">
  		<put-attribute name="content" value="/WEB-INF/views/{1}/{2}/{3}Detail.jsp" />
	</definition>


    <definition name="*/*" extends="base">
    	<put-attribute name="article" value="/WEB-INF/views/{1}/{2}.jsp" />
    </definition>


    <definition name="*/*/*" extends="base">
    	<put-attribute name="article" value="/WEB-INF/views/{1}/{2}/{3}.jsp" />
    </definition>

    <definition name="*/*/*/*" extends="base">
    	<put-attribute name="article" value="/WEB-INF/views/{1}/{2}/{3}/{4}.jsp" />
    </definition>

	<!-- 네비게이션 없이 호출되는 목록 -->





