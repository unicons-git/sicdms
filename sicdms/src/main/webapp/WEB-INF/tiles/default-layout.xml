<!DOCTYPE tiles-definitions PUBLIC
  "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
  "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>

	<!-- 화면의 기초가 되는 base -->
    <definition name="base" template="/WEB-INF/tiles/template.jsp">
    	<put-attribute name="include"   value="/WEB-INF/views/main/include.jsp" />
    	<put-attribute name="header" 	value="/WEB-INF/views/main/header.jsp" />
        <put-attribute name="nav"   	value="/WEB-INF/views/main/nav.jsp" />
        <put-attribute name="article"   value=""/>
        <put-attribute name="footer" 	value="/WEB-INF/views/main/footer.jsp" />
    </definition>

    <definition name="nonNavBase" template="/WEB-INF/tiles/nonNavTemplate.jsp">
    	<put-attribute name="include"   value="/WEB-INF/views/main/include.jsp" />
    	<put-attribute name="header" 	value="/WEB-INF/views/main/header.jsp" />
        <put-attribute name="article"   value=""/>
        <put-attribute name="footer" 	value="/WEB-INF/views/main/footer.jsp" />
    </definition>


	<!-- 외부사용자 로그인 페이지 -->
	<definition name="login/login" 	template="/WEB-INF/tiles/empty.jsp">
  		<put-attribute name="content" 	value="/WEB-INF/views/login/login.jsp" />
	</definition>


	<!-- eGovError 페이지 -->
	<definition name="sicims000Cm/egovError" template="/WEB-INF/tiles/empty.jsp">
		<put-attribute name="content" value="/WEB-INF/views/sicims000Cm/egovError.jsp" />
	</definition>


	<!-- 세션 타임아웃 페이지 -->
	<definition name="login/sessionTimeout" template="/WEB-INF/tiles/empty.jsp">
		<put-attribute name="content" value="/WEB-INF/views/login/sessionTimeout.jsp" />
	</definition>
	<definition name="sicims000Cm/sessionTimeout" template="/WEB-INF/tiles/empty.jsp">
		<put-attribute name="content" value="/WEB-INF/views/sicims000Cm/sessionTimeout.jsp" />
	</definition>

	<!-- 메시지 박스  -->
	<definition name="sicims000Cm/messageBox" template="/WEB-INF/tiles/empty.jsp">
		<put-attribute name="content" value="/WEB-INF/views/sicims000Cm/messageBox.jsp" />
	</definition>

 	<definition name="login/*" 			template="/WEB-INF/tiles/empty.jsp">
		<put-attribute name="content" 	value="/WEB-INF/views/login/{1}.jsp" />
	</definition>


	<!-- 카카오맵  -->
	<definition name="sicims000Cm/kakaoMap" template="/WEB-INF/tiles/empty.jsp">
		<put-attribute name="content" value="/WEB-INF/views/sicims000Cm/kakaoMap.jsp" />
	</definition>



	<!-- 출력  -->
	<definition name="sicims000Cm/rdPrint" template="/WEB-INF/tiles/popup.jsp">
		<put-attribute name="article" value="/WEB-INF/views/sicims000Cm/rdPrint.jsp" />
	</definition>

	<!-- popup  -->
	<definition name="*/*Popup" template="/WEB-INF/tiles/popup.jsp">
		<put-attribute name="article" 		value="/WEB-INF/views/{1}/{2}Popup.jsp" />
	</definition>
	<definition name="*/*/*Popup" template="/WEB-INF/tiles/popup.jsp">
		<put-attribute name="article" 		value="/WEB-INF/views/{1}/{2}/{3}Popup.jsp" />
	</definition>

	<!-- popup  -->
